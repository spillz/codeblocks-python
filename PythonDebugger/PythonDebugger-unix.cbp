<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PythonDebugger-Unix" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/libPythonDebugger.so" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-ansi" />
					<Add option="-g" />
					<Add directory="$(#cb.include)" />
					<Add directory="$(#cb)/sdk/wxscintilla/include" />
				</Compiler>
				<Linker>
					<Add library="codeblocks" />
					<Add directory="$(#cb)/devel" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 $(#cb)/devel/share/codeblocks/PythonDebugger.zip manifest.xml Resources/debugger_options_dlg.xrc" />
				</ExtraCommands>
			</Target>
			<Target title="cbplugin">
				<Option output="PythonDebugger.so" prefix_auto="0" extension_auto="0" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -ns -ni -p debug --prefix $(#cb)/devel/debug-data" />
				<Option host_application="$(#cb)/devel/codeblocks" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-ansi" />
					<Add option="-g" />
					<Add option="`pkg-config --cflags codeblocks`" />
				</Compiler>
				<Linker>
					<Add option="`pkg-config --libs codeblocks`" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 PythonDebugger.zip manifest.xml Resources/debugger_options_dlg.xrc" />
					<Add after="zip -r9 PythonDebugger.cbplugin PythonDebugger.so PythonDebugger.zip" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="py" value="python2.5" />
				<Variable name="wx=$wx_LINUX" value="" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="`wx-config --cflags`" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-Ulinux" />
			<Add option="-Uunix" />
			<Add option="-fPIC" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
		</Compiler>
		<Linker>
			<Add option="`wx-config --libs`" />
		</Linker>
		<Unit filename="PythonDebugger.cpp" />
		<Unit filename="PythonDebugger.h" />
		<Unit filename="Resources/debugger_options_dlg.xrc" />
		<Unit filename="debuggeroptionsdlg.cpp" />
		<Unit filename="debuggeroptionsdlg.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
