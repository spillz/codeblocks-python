<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="PythonInterpreter" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="$(#cb)/devel/share/codeblocks/plugins/PythonInterpreter" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p debug" />
				<Option host_application="$(#cb)/devel/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-D_WINDOWS" />
					<Add directory="$(#cb.include)" />
					<Add directory="$(#cb.include)/tinyxml" />
					<Add directory="$(#cb)/sdk/wxscintilla/include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)/gcc_dll/mswu" />
					<Add directory="../XmlRpcEmbedder/XMLRPC/include" />
					<Add directory="../XmlRpcEmbedder" />
				</Compiler>
				<Linker>
					<Add library="wxmsw28u" />
					<Add library="codeblocks" />
					<Add library="XmlRpcEmbedder" />
					<Add directory="$(#cb)/devel" />
					<Add directory="$(#wx.lib)/gcc_dll" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 PythonInterpreter.zip manifest.xml" />
					<Add after="cmd /c xcopy /y PythonInterpreter.zip $(#cb)\devel\share\codeblocks\" />
					<Add after="cmd /c xcopy /y python\interp.py $(#cb)\devel\share\codeblocks\python\" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="cblugin-toolspluslink">
				<Option output="PythonInterpreter" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p debug" />
				<Option host_application="$(#cb)/devel/codeblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DcbDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DTOOLSPLUSLINK" />
					<Add option="-D_WINDOWS" />
					<Add directory="$(#cb.include)" />
					<Add directory="$(#cb)/sdk/wxscintilla/include" />
					<Add directory="$(#wx.include)" />
					<Add directory="$(#wx.lib)/gcc_dll/mswu" />
					<Add directory="$(#cb)/plugins/contrib/ToolsPlus" />
					<Add directory="../XmlRpcEmbedder/XMLRPC/include" />
					<Add directory="../XmlRpcEmbedder" />
				</Compiler>
				<Linker>
					<Add library="wxmsw28u" />
					<Add library="codeblocks" />
					<Add library="XmlRpcEmbedder" />
					<Add library="$(#cb)/devel/share/CodeBlocks/plugins/ToolsPlus.dll" />
					<Add directory="$(#cb)/devel" />
					<Add directory="$(#wx.lib)/gcc_dll" />
					<Add directory="$(#cb)/devel/share/CodeBlocks/plugins" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 PythonInterpreter.zip manifest.xml" />
					<Add after="zip -r9 PythonInterpreter.cbplugin PythonInterpreter.zip PythonInterpreter.dll python/interp.py" />
					<Mode after="always" />
				</ExtraCommands>
			</Target>
			<Target title="cbplugin">
				<Option output="PythonInterpreter" prefix_auto="1" extension_auto="1" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --no-dde --no-check-associations --multiple-instance --no-splash-screen --verbose -p debug" />
